{
  "name": "blm-spider",
  "version": "1.0.0",
  "description": "Scrape BLM dispersed campsite data by crawling their official site (starting at search results), parsing each campsite detail page, and saving structured data to JSON.",
  "main": "dist/api/resources.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "mkdir -p dist/api && cp src/api/*.js src/api/*.graphql dist/api/",
    "start": "harperdb run .",
    "cron": "node dist/core/jobs/cron.js",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "node-cron": "^4.2.1",
    "p-limit": "^6.2.0",
    "uuid": "^11.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@vitest/ui": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
